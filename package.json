{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "eslint . --ext .ts",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prettify": "npx prettier '**/*.{json,sol,md,ts}' --write",
    "typecheck": "tsc --noEmit",
    "audit": "yarn audit --severity=high"
  },
  "peerDependencies": {
    "ethers": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint . --ext .ts"
    }
  },
  "name": "pnsjs",
  "author": "Justice Eziefule",
  "description": "A Typescript library for interacting with the PNS smart contract",
  "module": "dist/pnsjs.esm.js",
  "size-limit": [
    {
      "path": "dist/pnsjs.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/pnsjs.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "axios": "^1.3.3",
    "ethers": "^5.6.1",
    "web3-utils": "^1.8.1"
  },
  "devDependencies": {
    "@size-limit/esbuild": "^8.2.4",
    "@size-limit/esbuild-why": "^8.2.4",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.4",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  }
}
